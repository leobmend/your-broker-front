{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "trybe-frontend",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "react/no-did-mount-set-state": "off",
        "react/jsx-max-depth": "off"
    }
}